{% extends 'base.html.twig' %}

{% block title %}
List of all quizz
{% endblock %}

{% block body %}
    <table>
        <thead>
            <tr>
                <th>Theme</th>
                <th>Creator username</th>
            </tr>
        </thead>
        <tbody>
            {% for quizz in allQuizz %}
                <tr>
                    <td>{{ quizz.theme }}</td>
                    <td>{{ quizz.creator.username }}</td>
                </tr>
            {% endfor %}
        </tbody>
{% endblock %}
